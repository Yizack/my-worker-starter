{
  "private": true,
  "name": "my-worker-starter",
  "description": "My starter for Cloudflare workers",
  "author": "Yizack Rangel",
  "type": "module",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "postinstall": "pnpm dev:prepare",
    "dev": "pnpm prepare && wrangler dev",
    "dev:prepare": "pnpm tsx scripts/routes.ts",
    "worker": "pnpm prepare && wrangler deploy",
    "lint": "pnpm lint:eslint --fix",
    "lint:eslint": "eslint",
    "test:types": "tsc --noEmit"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240712.0",
    "@eslint/compat": "^1.1.1",
    "@stylistic/eslint-plugin": "^2.3.0",
    "@types/node": "^20.14.10",
    "@typescript-eslint/parser": "^7.16.0",
    "eslint": "^9.6.0",
    "h3": "^1.12.0",
    "ofetch": "^1.3.4",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "wrangler": "3.64.0"
  },
  "packageManager": "pnpm@9.5.0"
}
